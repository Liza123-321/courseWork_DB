@{
    ViewBag.Title = "Map";
}
<link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
<div class="container-fluid" marging="0" padding="0" width="100%">
    <iframe width="100%" height="800px" marging="0" padding="0" src="https://www.arcgis.com/apps/StoryMapBasic/index.html?appid=ac606a1ac19e43629184ec2116ca480c" frameborder="0" scrolling="no"></iframe>
</div>
<br/>
<div class="container-fluid">
    <div class="readmore-w3 container">@Html.ActionLink("Добавить местоположение", "AddPoint", "Map")</div>
</div><br /><br />
<div style="background: Gainsboro; height: 50px;" class="panel panel-info">
    <h1>Местоположения</h1>
</div>

<h3>
    <table class="table">
        <tr>
            <td>Id</td>
            <td>Тип</td>
            <td>Координаты</td>
        </tr>
        @foreach (var b in ViewBag.CoordRe)
        {
            <tr>
                <td>@b.Id </td>
                <td>@b.Type</td>
                <td>@b.Coord</td>

            </tr>
        }
    </table>
</h3><br /><br />
